{
    "name": "rbrauner/apilo-recruitment-task",
    "description": "Apilo recruitment task",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "rbrauner",
            "email": "rafalbrauner@gmail.com"
        }
    ],
    "require": {
        "php": "8.3.9",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/composer": "^2.7.7",
        "phpdocumentor/reflection-docblock": "^5.4.1",
        "symfony/console": "^6.4.8",
        "symfony/dotenv": "^6.4.8",
        "symfony/flex": "^2.4.5",
        "symfony/framework-bundle": "^6.4.9",
        "symfony/http-client": "^6.4.8",
        "symfony/messenger": "^6.4.9",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "^6.4.8",
        "symfony/runtime": "^6.4.8",
        "symfony/serializer": "^6.4.9",
        "symfony/validator": "^6.4.9",
        "symfony/yaml": "^6.4.8"
    },
    "require-dev": {
        "enlightn/security-checker": "^1.11.0",
        "ergebnis/composer-normalize": "^2.43.0",
        "friendsofphp/php-cs-fixer": "^3.59.3",
        "phpro/grumphp-shim": "^2.6.0",
        "phpstan/extension-installer": "^1.4.1",
        "phpstan/phpstan": "^1.11.7",
        "phpstan/phpstan-phpunit": "^1.4.0",
        "phpunit/phpunit": "^11.2.7",
        "rector/rector": "^1.2.1",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.10.1",
        "symfony/debug-bundle": "^6.4.8",
        "symfony/maker-bundle": "^1.60.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "phpro/grumphp-shim": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.4.8"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
